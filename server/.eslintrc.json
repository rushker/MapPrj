{
    "env": {
        "browser": false,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": [
            "error"
        ],
        "no-console": "warn"
    }
}